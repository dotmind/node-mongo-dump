{
  "name": "@dotmind/node-mongo-dump",
  "version": "1.0.2",
  "description": "An npm package to generate database dump with customizable node cron",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/node-cron": "2.0.3",
    "child_process": "1.0.2",
    "compressing": "1.5.1",
    "node-bin-setup": "1.0.6",
    "node-cron": "2.0.3",
    "opencollective-postinstall": "2.0.3",
    "semver": "^5.7.1",
    "tz-offset": "0.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@types/node": "14.14.10",
    "@types/semver": "^7.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotmind/node-mongo-dump.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "node",
    "mongo",
    "dump"
  ],
  "author": "Vincent",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dotmind/node-mongo-dump/issues"
  },
  "homepage": "https://github.com/dotmind/node-mongo-dump#readme"
}
